              tools:context="org.xerrard.deviceparam.MainActivity"
              tools:deviceIds="wear_round">

    <TextView android:id="@id/tvew_resolution" android:layout_width="wrap_content"
              android:layout_height="wrap_content" android:textSize="@dimen/tvew_size"/>
