<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
    android:id="@+id/frame_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background">
    <!--app:layout_box="left|top|bottom|right"-->
    <com.igeak.record.WearableListView
        android:id="@+id/wearable_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </com.igeak.record.WearableListView>

    <RelativeLayout
        android:id="@+id/item_img_rl"
        android:layout_width="match_parent"
        android:layout_height="99px"

    >

        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                  android:layout_centerInParent="true"
                  android:text="@string/record_list"
                  android:textColor="@color/list_head_color"
                  android:textSize="@dimen/list_head_text_size"
        />
    </RelativeLayout>


</FrameLayout>

